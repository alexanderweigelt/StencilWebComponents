{
  "name": "ui",
  "version": "0.0.1",
  "description": "UI components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ui/ui.esm.js",
  "exports": {
    ".": {
      "import": "./dist/ui/ui.esm.js",
      "require": "./dist/ui/ui.cjs.js"
    },
    "./ui-headline": {
      "import": "./dist/components/ui-headline.js",
      "types": "./dist/components/ui-headline.d.ts"
    },
    "./ui-button": {
      "import": "./dist/components/ui-button.js",
      "types": "./dist/components/ui-button.d.ts"
    },
    "./ui-main": {
      "import": "./dist/components/ui-main.js",
      "types": "./dist/components/ui-main.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  }
}
